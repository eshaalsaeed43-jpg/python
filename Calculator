import tkinter as tk
root= tk.Tk()
root.title("calculator")
root.iconbitmap("calculator.ico")
root.geometry("400x400")
entry1= tk.Entry(root, width=12, borderwidth=5, font=("Arial",40))
entry1.grid(row=0, column=0, padx=20,pady=40)
button1= tk.Button(root, text="1",font=("Arial", 30))
button1.grid(row=1, column=0)
button2= tk.Button(root, text="1",font=("Arial",30))
button2.grid(row=1, column=1)

root.mainloop()
